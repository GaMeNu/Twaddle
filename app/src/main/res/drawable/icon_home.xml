<?xml version="1.0" encoding="utf-8"?>
<animated-selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@+id/icon_home_checked"
        android:drawable="@drawable/baseline_home" android:state_checked="true"
        android:name="icon_home_checked"/>
    <item android:id="@+id/icon_home_default"
        android:drawable="@drawable/outline_home"/>

    <transition
        android:fromId="@id/icon_home_default"
        android:toId="@id/icon_home_checked">
        <animated-vector
            xmlns:aapt="http://schemas.android.com/aapt">
            <aapt:attr name="android:drawable">
                <vector
                    android:name="vector"
                    android:width="24dp"
                    android:height="24dp"
                    android:viewportWidth="24"
                    android:viewportHeight="24">
                    <path
                        android:name="path"
                        android:pathData="M 12 5.69 L 17 10.19 L 17 18 L 15 18 L 15 12 L 9 12 L 9 18 L 7 18 L 7 10.19 L 12 5.69 M 12 3 L 2 12 L 5 12 L 5 20 L 11 20 L 11 14 L 13 14 L 13 20 L 19 20 L 19 12 L 22 12 L 12 3 Z"/>
                </vector>
            </aapt:attr>
            <target android:name="path">
                <aapt:attr name="android:animation">
                    <objectAnimator
                        android:propertyName="pathData"
                        android:duration="100"
                        android:valueFrom="M 9 12 L 15 12 L 15 18 L 17 18 L 17 10.19 L 12 5.69 L 7 10.19 L 7 18 L 9 18 L 9 12 M 12 3 L 2 12 L 5 12 L 5 20 L 11 20 L 11 14 L 13 14 L 13 20 L 19 20 L 19 12 L 22 12 L 12 3 Z"
                        android:valueTo="M 12 12 L 12 12 L 12 12 L 12 12 L 12 12 L 12 12 L 12 12 L 12 12 L 12 12 L 12 12 M 12 3 L 2 12 L 5 12 L 5 20 L 11 20 L 11 14 L 13 14 L 13 20 L 19 20 L 19 12 L 22 12 L 12 3 Z"
                        android:valueType="pathType"
                        android:interpolator="@android:interpolator/fast_out_slow_in"/>
                </aapt:attr>
            </target>
        </animated-vector>
    </transition>
</animated-selector>